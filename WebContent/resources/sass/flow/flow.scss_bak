	text-align:center;
	line-height:20px;
	color:$gray-dark;
}
.checkstep {
	height: 200px;
}
.nofile {
	margin-left: 240px;
}
.nomessage {
	margin-left: 240px;
	margin-top: 30px;
	margin-bottom: 50px;
}

