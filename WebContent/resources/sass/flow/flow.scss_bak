	height: 100%;
}
.sure-margin {
	margin-top: 100px;
	margin-left: 140px;
}
.cancle-margin {
	margin-top: 100px;
	margin-left:42px;
}
.nav .logo {
	position: relative;
	float: left;
	top: 14px;
	background: url(../../images/common/logo.svg) no-repeat;
	width: 92px;
	height: 30px;
	margin-right: 76px;
}
.check-step {

    width:400px;
	color: $gray;
	font-size: $font-size-lg;
	position: absolute;
	top: 50px;
